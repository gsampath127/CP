@using RRD.FSG.RP.Web.UI.Hosted;

<link href="@(((BaseViewModel)Model).BaseURL)PageTextHandler.ashx?ClientName=@(((BaseViewModel)Model).ClientName)&SiteName=@(((BaseViewModel)Model).SiteName)&Key=@(((BaseViewModel)Model).PageCSSResourceKey)&IsProofing=@(((BaseViewModel)Model).IsProofing)&PageId=@(((BaseViewModel)Model).PageId)" rel="stylesheet" media="all" />

<html>
<body class="cssTaxonomyDataNotAvailableBody">
    <div class="cssTaxonomyDataNotAvailableMainDiv">
        <div class="cssHeaderTable">
            <div class="cssHeaderTableRow">
                <div class="cssHeaderTableCell cssClientLogoPlaceHolder">
                    <div class="cssClientLogo">@(Html.Raw(System.Web.HttpUtility.HtmlDecode(((BaseViewModel)Model).LogoText)))</div>
                </div>
            </div>
        </div>

        <div class="cssTaxonomyDataNotAvailableItemDiv">
            <div class="cssTaxonomyDataNotAvailableTextDiv">@(Html.Raw(System.Web.HttpUtility.HtmlDecode(((BaseViewModel)Model).TaxonomyNotAvailableText)))</div>
            <div class="cssTaxonomyDataNotAvailableGoBackButtonDiv">
                <input type="submit" name="btnGoBack" value="Go Back" id="btnGoBack" onclick="window.history.back()">
            </div>
        </div>

    </div>
</body>
</html>
